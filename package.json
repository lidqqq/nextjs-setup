{
	"name": "nextjs-setup",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "run-p dev:*",
		"dev:next": "next dev",
		"dev:path": "pathpida --ignorePath .gitignore --watch",
		"build": "pathpida --ignorePath .gitignore && next build",
		"start": "next start",
		"lint": "gts lint",
		"compile": "tsc",
		"fix": "gts fix",
		"test": "vitest",
		"test:ci": "vitest run",
		"coverage": "vitest run --coverage",
		"doctor": "./scripts/doctor.sh",
		"lintstaged": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"jotai": "1.7.3",
		"lodash": "4.17.21",
		"next": "12.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-use": "17.4.0",
		"stop-runaway-react-effects": "2.0.0",
		"type-fest": "2.15.1"
	},
	"devDependencies": {
		"@testing-library/react": "13.3.0",
		"@types/node": "18.0.0",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@vitejs/plugin-react": "1.3.2",
		"autoprefixer": "10.4.7",
		"c8": "7.11.3",
		"eslint": "8.19.0",
		"eslint-config-next": "12.2.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"gts": "^3.1.0",
		"happy-dom": "6.0.0",
		"husky": "8.0.1",
		"lint-staged": "13.0.3",
		"npm-run-all": "4.1.5",
		"pathpida": "0.18.1",
		"postcss": "8.4.14",
		"tailwindcss": "3.1.4",
		"typescript": "4.7.4",
		"vitest": "0.16.0"
	}
}
